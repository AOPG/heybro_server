            <!--<columnOverride column="login_time" jdbcType="TIMESTAMP" javaType="java.time.LocalDateTime"/>-->
            <!--<columnOverride column="reg_time" jdbcType="TIMESTAMP" javaType="java.time.LocalDateTime"/>-->
